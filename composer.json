{
	"name": "modette/code-generator",
	"description": "Generate code with simple commands",
	"type": "library",
	"license": "MPL-2.0",
	"keywords": [
		"cli",
		"console",
		"generator"
	],
	"homepage": "https://github.com/modette/cmf",
	"authors": [
		{
			"name": "Marek Barto≈°",
			"homepage": "https://marek-bartos.cz"
		}
	],
	"require": {
		"php": "^7.2.0"
	},
	"require-dev": {
		"phpstan/extension-installer": "^1.0.0",
		"phpstan/phpstan": "^0.11.0",
		"phpstan/phpstan-nette": "^0.11.0",
		"phpstan/phpstan-strict-rules": "^0.11.0",
		"phpunit/phpunit": "^8.2.3"
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"autoload": {
		"psr-4": {
			"Modette\\CodeGenerator\\": "src"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\Modette\\CodeGenerator\\": "tests"
		}
	},
	"config": {
		"sort-packages": true
	},
	"extra": {
		"branch-alias": {
			"dev-master": "1.0.x-dev"
		}
	}
}
